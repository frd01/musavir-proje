        const muk_sgk_txa_adres = $("#muk_sgk_txa_adres").data("kendoTextArea").value()

        const muk_sgk_dt_acilis_tarihi = $("#muk_sgk_dt_acilis_tarihi").data("kendoDatePicker").value()

        const muk_sgk_dt_kapanis_tarihi = $("#muk_sgk_dt_kapanis_tarihi").data("kendoDatePicker").value()

        const muk_sgk_ch_aktif = $("#muk_sgk_ch_aktif").attr("checked")

        if (tx_sgk_sube_adi.length<=0) {

            alert("Şube Adını Giriniz.")
            return
        }
        if (tx_sgk_kullanici_adi_1.length <= 0 || tx_sgk_kullanici_adi_2.length <=0) {

            alert("Kullanıcı Adını Giriniz.")
            return
        }
        if (tx_sgk_sistem_sifresi.length <=0) {

            alert("Sistem Şifresini Giriniz.")
            return
        }
        if (tx_sgk_isyeri_sifresi.length <=0) {

            alert("İşyeri Şifresini Giriniz.")
            return
        }

        if (muk_sgk_ch_aktif != null) {

            sube_model.Aktif = muk_sgk_ch_aktif
        }

        if (muk_sgk_dt_acilis_tarihi != null) {

            sube_model.AcilisTarihi = tarih_func_sql(muk_sgk_dt_acilis_tarihi)
        }

        if (muk_sgk_dt_kapanis_tarihi != null) {

            sube_model.KapanisTarihi = tarih_func_sql(muk_sgk_dt_kapanis_tarihi)
        }

        if (tx_sgk_sube_adi.length >0 ) {

            sube_model.SubeAdi = tx_sgk_sube_adi
        }
        if (tx_sgk_kullanici_adi_1.length>0) {

            sube_model.KullaniciAdi_1 = tx_sgk_kullanici_adi_1
        }
        if (tx_sgk_kullanici_adi_2.length > 0) {
            sube_model.KullaniciAdi_2 = tx_sgk_kullanici_adi_2
        }
        if (tx_sgk_sistem_sifresi.length >0) {

            sube_model.SistemSifresi = tx_sgk_sistem_sifresi
        }
        if (tx_sgk_isyeri_sifresi.length>0) {

            sube_model.IsyeriSifresi = tx_sgk_isyeri_sifresi
       }
       console.log("muk_sgk_sube_txa_notlar : ", muk_sgk_sube_txa_notlar)
        if (muk_sgk_sube_txa_notlar != null) {

            sube_model.Notlar = muk_sgk_sube_txa_notlar
        }

        if (muk_sgk_txa_adres != null) {

            sube_model.Adres = muk_sgk_txa_adres
       }

       const sgk_m_kod = $("#sgk_m_kod").data("kendoTextBox").value()
       const sgk_is_kolu = $("#sgk_is_kolu").data("kendoTextBox").value()
       const sgk_yeni = $("#sgk_yeni").data("kendoTextBox").value()
       const sgk_eski = $("#sgk_eski").data("kendoTextBox").value()
       const sgk_isyeri_sira_no = $("#sgk_isyeri_sira_no").data("kendoTextBox").value()
       const sgk_il_kodu = $("#sgk_il_kodu").data("kendoTextBox").value()
       const sgk_ilce_kodu = $("#sgk_ilce_kodu").data("kendoTextBox").value()
       const sgk_kontrol_no = $("#sgk_kontrol_no").data("kendoTextBox").value()
       const sgk_araci_kodu = $("#sgk_araci_kodu").data("kendoTextBox").value()

       if (sgk_m_kod.length <= 0) {
           alert("Sgk M Kodunu Giriniz.")
           return
       }
       if (sgk_is_kolu.length <=0) {

           alert("Sgk İş Kolu Kodunu Giriniz.")
           return
       }

       if (sgk_yeni.length <= 0) {
           alert("Sgk Yeni Kodunu Giirniz.")
           return
       }
       if (sgk_eski.length <= 0) {

           alert("Sgk Eski Kodunu Giriniz.")
           return
       }
       if (sgk_isyeri_sira_no.length <=0) {

           alert("Sgk İşyeri Sıra No'yu Giriniz.")
           return
       }

       if (sgk_il_kodu.length <=0) {

           alert("Sgk İl Kodunu Giriniz.")
           return
       }
       if (sgk_ilce_kodu.length <=0) {

           alert("Sgk İlkodunuz Giriniz.")
           return
       }
       if (sgk_kontrol_no.length<=0) {

           alert("Sgk Kontrol No'yu Giriniz.")
           return
       }
       if (sgk_araci_kodu.length <=0) {

           alert("Sgk Aracı Kodunu Giriniz.")
           return
       }

       sube_model.M = sgk_m_kod
       sube_model.IsKoluKodu = sgk_is_kolu
       sube_model.Yeni = sgk_yeni
       sube_model.Eski = sgk_eski
       sube_model.IsYeriSiraNo = sgk_isyeri_sira_no
       sube_model.IlKodu = sgk_il_kodu
       sube_model.IlceKodu = sgk_ilce_kodu
       sube_model.KontrolNo = sgk_kontrol_no
       sube_model.AraciKodu = sgk_araci_kodu
      

       sube_model.TakipListesi = bilgi_takip_listesi

       const firmaId = $("#ID").val()

       kendo.ui.progress($('#content_div_id'), true);

       var result = await $.ajax({

           url: "@Url.Action("MukellefSgkSubeKaydet", "mukellef")",
           method: "POST",
           dataType: "Json",
           data: { clientData: sube_model, firmaId: firmaId }

       })

       if (result.IslemDurum == false) {

           alert(result.Mesaj)
           return
       }

       await sgk_sube_grid_yenile()
       kendo.ui.progress($('#content_div_id'), false);
        sube_model_temizle()

        const wn_muk_yeni_sube_ekle = $("#wn_muk_yeni_sube_ekle").data("kendoWindow")

        wn_muk_yeni_sube_ekle.close()
    }

</script>
