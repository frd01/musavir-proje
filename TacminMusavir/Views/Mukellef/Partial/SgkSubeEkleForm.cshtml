        $("#muk_sgk_ch_aktif").prop("checked", sube_model.Aktif)
    }

    function sgk_takip_list_durum_guncelle(durum,btnId) {

        for (var i = 0; i < bilgi_takip_listesi.length; i++) {

            if (bilgi_takip_listesi[i].BtnId == btnId) {

                bilgi_takip_listesi[i].IsSecili = durum
            }
        }
    }

    function takip_secim_click_event(_value) {

        let value = _value.toString().split(':')

        const btn_id = "#" + value[0]
        const icon_id = "#" + value[1]
        const btn = $(btn_id)
        const icon = $(icon_id)

        if (btn.hasClass("btn btn-danger") == true) {

            btn.removeClass("btn btn-danger")
            btn.addClass("btn btn-success")

            icon.removeClass("fa-solid fa-xmark")
            icon.addClass("fa-solid fa-check")

            sgk_takip_list_durum_guncelle(true, btn_id)


        }
        else {

            btn.removeClass("btn btn-success")
            btn.addClass("btn btn-danger")

            icon.removeClass("fa-solid fa-check")
            icon.addClass("fa-solid fa-xmark")

            sgk_takip_list_durum_guncelle(false, btn_id)
            
        }
    }

    function get_id() {

        let u = '', i = 0;
        while (i++ < 36) {
            var c = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'[i - 1], r = Math.random() * 16 | 0, v = c == 'x' ? r : (r & 0x3 | 0x8);
            u += (c == '-' || c == '4') ? c : v.toString(16)
        }
        return u;
    }

   async function btn_sube_ekle_click_event() {

        const tx_sgk_sube_adi = $("#tx_sgk_sube_adi").data("kendoTextBox").value()
        const tx_sgk_kullanici_adi_1 = $("#tx_sgk_kullanici_adi_1").data("kendoTextBox").value()
        const tx_sgk_kullanici_adi_2 = $("#tx_sgk_kullanici_adi_2").data("kendoTextBox").value()
